{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "Onboard BIG-IP",
    "Common": {
        "class": "Tenant",
        "myLicense": {
            "class": "License",
            "licenseType": "licensePool",
            "bigIqHost": "${bigiq_hostname}",
            "bigIqUsername": "${bigiq_username}",
            "bigIqPassword": "${bigiq_password}",
            "licensePool": "${licensePool}",
            "skuKeyword1": "${skuKeyword1}",
            "unitOfMeasure": "${unitOfMeasure}",
            "reachable": "${reachable}",
            "bigIpUsername": "${bigIpUsername}",
            "bigIpPassword": "${bigIpPassword}",
            "hypervisor": "${hypervisor}"
        },        
        "myDns": {
            "class": "DNS",
            "nameServers": [
                "${nameserver}"
            ],
            "search": [
                "f5.com"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "0.pool.ntp.org","1.pool.ntp.org","2.pool.ntp.org"
            ],
            "timezone": "UTC"
        },
        "myProvisioning": {
            "class": "Provision",
            "ltm": "nominal",
            "asm": "nominal"
        },
        "external": {
            "class": "VLAN",
            "tag": 10,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": false
                }
            ]
        },
        "internal": {
            "class": "VLAN",
            "tag": 20,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.2",
                    "tagged": false
                }
            ]
        },
        "internal-self": {
            "class": "SelfIp",
            "address": "10.30.0.10/24",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "external-self": {
            "class": "SelfIp",
            "address": "10.20.0.9/24",
            "vlan": "external",
            "allowService": [
                "tcp:443"
            ],
            "trafficGroup": "traffic-group-local-only"
        }
    }
}